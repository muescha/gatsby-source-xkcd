{
  "name": "gatsby-source-xkcd",
  "version": "0.0.2",
  "description": "Gatsby source plugin for building websites with XKCD as a data source",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "author": "Stephen Weiss <stephencweiss@gmail.com> (https://stephencharlesweiss.com)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "xkcd"
  ],
  "peerDependencies": {
    "gatsby": "^2.2.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@types/node": "^13.7.1",
    "axios": "^0.19.2",
    "babel-preset-gatsby-package": "^0.2.16",
    "cross-env": "^7.0.0"
  }
}
